  "servers": [
    {
      "description": "Posque RESTful API Server",
      "url": "https://{environment}.aldi.com/api/v1",
      "variables": {
        "environment": {
          "default": "dev",
          "description": "Server environment",
          "enum": [
            "dev",
            "prod"
          ]
        }
      }
    }
  ],
  "paths": {
    "/login": {
      "post": {
        "tags": ["Authentication"],
        "summary": "Login to user account",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["email", "password"],
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email"
                  },
                  "password": {
                    "type": "string",
                    "format": "password"
                  }
                }
              },
              "example": {
                "email": "test@app.id",
                "password": "f8297e6cd0ae1b1e8cff4431d77e0ae80389aa4075b137156c9665fd199641e0"
              }
            }
          }
        },
        "responses": {
          "200":{
            "description": "",
            "content": {
              "application/json": {}
            }
          } 
        }
      }
    }
  },
  "components": {}
}